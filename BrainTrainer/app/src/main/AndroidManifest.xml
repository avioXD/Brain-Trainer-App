<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.braintrainer">

    <application
        android:allowBackup="true"
        android:icon="@drawable/applogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BrainTrainer"
        >
        <activity android:name=".Game_Score_Activity"></activity>
        <activity android:name=".Select_Game_Activity"/>
        <activity android:name=".Game_Board_Activity"/>
        <activity android:name=".Go_Activity"/>
        <activity android:name=".Game_Over_Activity"></activity>
        <activity android:name=".Home_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />


    </application>

</manifest>